<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="301.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.MyTunes.GUI.AddPlaylistController">
   <children>
      <Label layoutX="98.0" layoutY="77.0" text="Name your new playlist">
         <font>
            <Font name="Times New Roman Bold" size="40.0" />
         </font>
      </Label>
      <TextField fx:id="txtName" layoutX="75.0" layoutY="146.0" prefHeight="26.0" prefWidth="450.0" promptText="type name here">
         <font>
            <Font name="Times New Roman" size="14.0" />
         </font>
      </TextField>
      <Button layoutX="249.0" layoutY="204.0" mnemonicParsing="false" onAction="#createPlaylist" text="Create Playlist">
         <font>
            <Font name="Times New Roman" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
